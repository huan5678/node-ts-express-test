{
  "compilerOptions": {
    "module": "commonjs",
    "outDir": "dist",
    "target": "esnext",
    "moduleResolution": "node",
    "jsx": "react",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "strict": true,
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "sourceMap": true,
  },
  "lib": [
    "es2015"
  ],
  "include": [
    "src",
  ],
  "exclude": [
    "node_modules",
    "dist",
  ]
}